<Window
	x:Class="EOToolsWeb.Views.MainWindow"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:views="clr-namespace:EOToolsWeb.Views"
	xmlns:vm="using:EOToolsWeb.ViewModels"
	Title="EOToolsWeb"
	d:DesignHeight="450"
	d:DesignWidth="800"
	x:DataType="vm:MainViewModel"
	Icon="/Assets/avalonia-logo.ico"
	mc:Ignorable="d">
	<Panel>
		<Button Command="{Binding OpenUpdatesCommand}" Content="Updates" />
		<ContentPresenter x:Name="MainContent" />
	</Panel>
</Window>
